{
  "private": true,
  "sideEffects": false,
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "scripts": {
    "clean": "rimraf node_modules .cache .mf build dist functions",
    "generate-client": "prisma generate --data-proxy",
    "build": "remix build",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public",
    "dev": "remix build && run-p \"dev:*\"",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler",
    "postinstall": "remix-esbuild-override && remix setup cloudflare && npm run generate-client"
  },
  "dependencies": {
    "@aws-sdk/types": "^3.198.0",
    "@chakra-ui/icons": "^2.0.11",
    "@chakra-ui/react": "^2.3.6",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@prisma/client": "^4.5.0",
    "@remix-run/cloudflare-pages": "^1.7.4",
    "@remix-run/react": "^1.7.4",
    "@remix-validated-form/with-zod": "^2.0.2",
    "@types/exenv": "^1.2.0",
    "aws-sdk": "^2.1243.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "exenv": "^1.2.2",
    "framer-motion": "^7.6.1",
    "prisma": "^4.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-scroll": "^1.8.8",
    "remix": "^1.7.4",
    "remix-utils": "^3.4.0",
    "remix-validated-form": "^4.6.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@remix-run/dev": "^1.7.4",
    "@remix-run/eslint-config": "^1.7.4",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/react-scroll": "^1.8.4",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.26.0",
    "npm-run-all": "^4.1.5",
    "remix-esbuild-override": "^3.0.4",
    "typescript": "^4.8.4",
    "wrangler": "2.1.13"
  },
  "engines": {
    "node": ">=16"
  }
}
